{
    "workbench.startupEditor": "none",
    "editor.fontFamily": "'JetBrains Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.renderWhitespace": "boundary",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.parameterHints.cycle": true,
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.renderLineHighlightOnlyWhenFocus": true,
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.openPositioning": "last",
    "window.menuBarVisibility": "toggle",
    "debug.allowBreakpointsEverywhere": true,
    "terminal.external.linuxExec": "kitty",
    "terminal.integrated.cursorBlinking": true,
    "git.autofetch": true,
    "telemetry.enableTelemetry": false,
    "update.showReleaseNotes": false,
    "telemetry.enableCrashReporter": false,
    "git.alwaysShowStagedChangesResourceGroup": true,
    "editor.rulers": [
    80
    ],
    "epitech-c-cpp-headers.username": "thomas2.rancoule@epitech.eu",
    "epitech-c-cpp-headers.login": "thomas2.rancoule@epitech.eu",
    "epitech-c-cpp-headers.headerType": "post2017",
    "epitech-c-cpp-headers.usePragmaOnce": false,
    "epitech-c-cpp-headers.autoGenerateClasses": true,
    "epitech-c-cpp-headers.indentAccessSpecifiers": false,
    "workbench.colorTheme": "Dracula",
    "git.confirmSync": false,
    "workbench.iconTheme": "material-icon-theme"
}